import 'dart:io';

void main() {
  print("=== Multiplication Table Helper ===");

  stdout.write("Enter a number: ");
  int number = int.parse(stdin.readLineSync()!);

  print("\nMultiplication Table of $number:");
  int sum = 0;

  for (int i = 1; i <= 10; i++) {
    int result = number * i;
    print("$number x $i = $result");
    sum += result;
  }

  print("\nSum of all the multiples: $sum");
  print("Keep practicing and see you soon!");
}
